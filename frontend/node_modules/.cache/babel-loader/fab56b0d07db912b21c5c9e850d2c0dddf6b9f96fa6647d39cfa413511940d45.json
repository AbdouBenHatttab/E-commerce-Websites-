{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport UserService from '@/services/UserService';\nexport default {\n  name: 'AdminUsers',\n  data() {\n    return {\n      users: []\n    };\n  },\n  async created() {\n    await this.fetchUsers();\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await UserService.getAllUsers();\n        this.users = response.data;\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    },\n    async updateUser(user) {\n      try {\n        console.log('Sending update request for user:', user);\n        await UserService.updateUser(user.id, {\n          name: user.name,\n          email: user.email,\n          phoneNumber: user.phoneNumber,\n          address: user.address,\n          imageUrl: user.imageUrl,\n          role: user.role,\n          status: user.status\n        });\n        alert('User updated successfully!');\n      } catch (error) {\n        console.error('Error updating user:', error);\n        alert('Failed to update user.');\n      }\n    },\n    async deleteUser(id) {\n      try {\n        await UserService.deleteUser(id);\n        this.users = this.users.filter(user => user.id !== id);\n        alert('User deleted successfully!');\n      } catch (error) {\n        console.error('Error deleting user:', error);\n        alert('Failed to delete user.');\n      }\n    },\n    goBack() {\n      this.$router.go(-1);\n    }\n  }\n};","map":{"version":3,"names":["UserService","name","data","users","created","fetchUsers","methods","response","getAllUsers","error","console","updateUser","user","log","id","email","phoneNumber","address","imageUrl","role","status","alert","deleteUser","filter","goBack","$router","go"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\admin\\AdminUsers.vue"],"sourcesContent":["<template>\n  <div class=\"container py-4\">\n    <div class=\"card shadow-sm border-0\">\n      <div class=\"card-header bg-primary text-white py-3\">\n        <h3 class=\"mb-0\">\n          <i class=\"bi bi-people-fill me-2\"></i>User Management\n        </h3>\n      </div>\n      <div class=\"card-body p-3\">\n        <div class=\"table-responsive\">\n          <table class=\"table table-hover align-middle mb-0\">\n            <thead class=\"table-light\">\n              <tr>\n                <th class=\"ps-3\">ID</th>\n                <th>Name</th>\n                <th>Email</th>\n                <th>Phone</th>\n                <th>Address</th>\n                <!-- <th>Image</th> -->\n                <th>Role</th>\n                <th>Status</th>\n                <th class=\"text-end pe-3\">Actions</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr\n                v-for=\"user in users\"\n                :key=\"user.id\"\n                class=\"transition-effect\"\n              >\n                <td class=\"fw-semibold ps-3\">{{ user.id }}</td>\n                <td>\n                  <input\n                    v-model=\"user.name\"\n                    class=\"form-control form-control-sm focus-highlight\"\n                  />\n                </td>\n                <td class=\"text-truncate\" style=\"max-width: 200px\">\n                  {{ user.email }}\n                </td>\n                <td>\n                  <input\n                    v-model=\"user.phoneNumber\"\n                    class=\"form-control form-control-sm focus-highlight\"\n                    type=\"tel\"\n                  />\n                </td>\n                <td>\n                  <input\n                    v-model=\"user.address\"\n                    class=\"form-control form-control-sm focus-highlight\"\n                  />\n                </td>\n                <!-- <td>\n                  <input\n                    v-model=\"user.imageUrl\"\n                    class=\"form-control form-control-sm focus-highlight\"\n                    type=\"url\"\n                  />\n                </td> -->\n                <td>\n                  <select\n                    v-model=\"user.role\"\n                    class=\"form-select form-select-sm role-select\"\n                  >\n                    <option value=\"ROLE_USER\">User</option>\n                    <option value=\"ROLE_ADMIN\">Admin</option>\n                  </select>\n                </td>\n                <td>\n                  <select\n                    v-model=\"user.status\"\n                    class=\"form-select form-select-sm status-select\"\n                  >\n                    <option value=\"ACTIVE\">Active</option>\n                    <option value=\"INACTIVE\">Inactive</option>\n                  </select>\n                </td>\n                <td class=\"text-end pe-3\">\n                  <div class=\"d-flex gap-2 justify-content-end\">\n                    <button\n                      class=\"btn btn-sm btn-outline-success action-btn\"\n                      @click=\"updateUser(user)\"\n                    >\n                      <i class=\"bi bi-save me-1\"></i>Save\n                    </button>\n                    <button\n                      class=\"btn btn-sm btn-outline-danger action-btn\"\n                      @click=\"deleteUser(user.id)\"\n                    >\n                      <i class=\"bi bi-trash\"></i>\n                    </button>\n                  </div>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n\n        <div class=\"mt-4 d-flex justify-content-end\">\n          <button class=\"btn btn-outline-secondary\" @click=\"goBack\">\n            <i class=\"bi bi-arrow-left me-1\"></i>Back\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport UserService from '@/services/UserService';\n\nexport default {\n  name: 'AdminUsers',\n  data() {\n    return {\n      users: [],\n    };\n  },\n  async created() {\n    await this.fetchUsers();\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await UserService.getAllUsers();\n        this.users = response.data;\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    },\n    async updateUser(user) {\n      try {\n        console.log('Sending update request for user:', user);\n        await UserService.updateUser(user.id, {\n          name: user.name,\n          email: user.email,\n          phoneNumber: user.phoneNumber,\n          address: user.address,\n          imageUrl: user.imageUrl,\n          role: user.role,\n          status: user.status,\n        });\n        alert('User updated successfully!');\n      } catch (error) {\n        console.error('Error updating user:', error);\n        alert('Failed to update user.');\n      }\n    },\n    async deleteUser(id) {\n      try {\n        await UserService.deleteUser(id);\n        this.users = this.users.filter((user) => user.id !== id);\n        alert('User deleted successfully!');\n      } catch (error) {\n        console.error('Error deleting user:', error);\n        alert('Failed to delete user.');\n      }\n    },\n    goBack() {\n      this.$router.go(-1);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.card-header {\n  border-bottom: 3px solid rgba(255, 255, 255, 0.1);\n}\n\n.table-hover tbody tr {\n  border-bottom: 1px solid #f0f0f0;\n}\n\n.table-hover tbody tr:hover {\n  background-color: #f8f9fa;\n  transform: translateX(4px);\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);\n}\n\n.transition-effect {\n  transition: all 0.25s cubic-bezier(0.4, 0, 0.2, 1);\n}\n\n.focus-ring {\n  transition: all 0.2s ease;\n}\n\n.focus-ring:focus {\n  box-shadow: 0 0 0 3px rgba(13, 110, 253, 0.15) !important;\n  background-color: white !important;\n}\n\n.form-select-sm {\n  cursor: pointer;\n  a