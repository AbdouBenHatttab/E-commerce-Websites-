      console.error('Cart data is missing items.');\n      }\n    } catch (error) {\n      console.error('Error fetching cart data:', error);\n    }\n  },\n};\n</script>\n\n<style scoped>\n/* Add styling as needed */\n</style>\n"],"mappings":"AAkBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF;MACA,MAAMC,QAAO,GAAI,MAAML,KAAK,CAACM,GAAG,CAAC,WAAW,CAAC;MAC7CC,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEH,QAAQ,CAACJ,IAAI,CAAC;;MAEhD;MACA,IAAII,QAAQ,CAACJ,IAAG,IAAKI,QAAQ,CAACJ,IAAI,CAACE,KAAK,EAAE;QACxC,IAAI,CAACD,IAAG,GAAIG,QAAQ,CAACJ,IAAI,EAAE;MAC7B,OAAO;QACLM,OAAO,CAACE,KAAK,CAAC,6BAA6B,CAAC;MAC9C;IACF,EAAE,OAAOA,KAAK,EAAE;MACdF,OAAO,CAACE,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;IACnD;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}