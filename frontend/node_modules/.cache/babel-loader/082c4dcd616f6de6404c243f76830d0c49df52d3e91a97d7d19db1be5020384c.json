{"ast":null,"code":"import CartService from \"@/services/CartService\";\nimport AuthService from \"@/services/AuthService\";\nexport default {\n  name: \"ShoppingCart\",\n  data() {\n    return {\n      cartItems: []\n    };\n  },\n  created() {\n    this.fetchCart();\n  },\n  methods: {\n    fetchCart() {\n      const userId = AuthService.getCurrentUser().id;\n      CartService.getCart(userId).then(response => {\n        this.cartItems = response.data;\n      }).catch(error => {\n        console.error(\"Error fetching cart items:\", error);\n      });\n    },\n    removeFromCart(productId) {\n      const userId = AuthService.getCurrentUser().id;\n      CartService.removeFromCart(userId, productId).then(() => {\n        this.fetchCart();\n      }).catch(error => {\n        console.error(\"Error removing item from cart:\", error);\n      });\n    },\n    updateQuantity(productId, quantity) {\n      const userId = AuthService.getCurrentUser().id;\n      CartService.updateQuantity(userId, productId, quantity).then(() => {\n        this.fetchCart();\n      }).catch(error => {\n        console.error(\"Error updating item quantity:\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["CartService","AuthService","name","data","cartItems","created","fetchCart","methods","userId","getCurrentUser","id","getCart","then","response","catch","error","console","removeFromCart","productId","updateQuantity","quantity"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\user\\UserCart.vue"],"sourcesContent":["<template>\n  <div class=\"shopping-cart\">\n    <h2>Your Shopping Cart</h2>\n    <div v-if=\"cartItems.length === 0\">\n      <p>Your cart is empty.</p>\n    </div>\n    <div v-else>\n      <div v-for=\"item in cartItems\" :key=\"item.productId\" class=\"cart-item\">\n        <h3>{{ item.productName }}</h3>\n        <p>Quantity: {{ item.quantity }}</p>\n        <p>Price: ${{ item.price }}</p>\n        <button @click=\"removeFromCart(item.productId)\">Remove</button>\n        <button @click=\"updateQuantity(item.productId, item.quantity + 1)\">\n          +\n        </button>\n        <button\n          @click=\"updateQuantity(item.productId, item.quantity - 1)\"\n          :disabled=\"item.quantity <= 1\"\n        >\n          -\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CartService from \"@/services/CartService\";\nimport AuthService from \"@/services/AuthService\";\n\nexport default {\n  name: \"ShoppingCart\",\n  data() {\n    return {\n      cartItems: [],\n    };\n  },\n  created() {\n    this.fetchCart();\n  },\n  methods: {\n    fetchCart() {\n      const userId = AuthService.getCurrentUser().id;\n      CartService.getCart(userId)\n        .then((response) => {\n          this.cartItems = response.data;\n        })\n        .catch((error) => {\n          console.error(\"Error fetching cart items:\", error);\n        });\n    },\n    removeFromCart(productId) {\n      const userId = AuthService.getCurrentUser().id;\n      CartService.removeFromCart(userId, productId)\n        .then(() => {\n          this.fetchCart();\n        })\n        .catch((error) => {\n          console.error(\"Error removing item from cart:\", error);\n        });\n    },\n    updateQuantity(productId, quantity) {\n      const userId = AuthService.getCurrentUser().id;\n      CartService.updateQuantity(userId, productId, quantity)\n        .then(() => {\n          this.fetchCart();\n        })\n        .catch((error) => {\n          console.error(\"Error updating item quantity:\", error);\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.shopping-cart {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n  border: 1px solid #ccc;\n  border-radius: 10px;\n}\n.cart-item {\n  margin-bottom: 20px;\n}\n</style>\n"],"mappings":"AA2BA,OAAOA,WAAU,MAAO,wBAAwB;AAChD,OAAOC,WAAU,MAAO,wBAAwB;AAEhD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS