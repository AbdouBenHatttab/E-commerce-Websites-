plate>\n\n<script>\nimport AuthService from \"@/services/AuthService\";\n\nexport default {\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      message: \"\"\n    };\n  },\n  methods: {\n    async register() {\n      try {\n        await AuthService.register({ name: this.name, email: this.email, password: this.password });\n        this.message = \"Registration successful! Redirecting to login...\";\n        setTimeout(() => {\n          this.$router.push({ name: \"Login\" });\n        }, 2000);\n      } catch (err) {\n        this.message = \"Registration failed. Please try again.\";\n      }\n    }\n  }\n};\n</script>\n"],"mappings":";AA2BA,OAAOA,WAAU,MAAO,wBAAwB;AAEhD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAMP,WAAW,CAACO,QAAQ,CAAC;UAAEL,IAAI,EAAE,IAAI,CAACA,IAAI;UAAEC,KAAK,EAAE,IAAI,CAACA,KAAK;UAAEC,QAAQ,EAAE,IAAI,CAACA;QAAS,CAAC,CAAC;QAC3F,IAAI,CAACC,OAAM,GAAI,kDAAkD;QACjEG,UAAU,CAAC,MAAM;UACf,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;YAAER,IAAI,EAAE;UAAQ,CAAC,CAAC;QACtC,CAAC,EAAE,IAAI,CAAC;MACV,EAAE,OAAOS,GAAG,EAAE;QACZ,IAAI,CAACN,OAAM,GAAI,wCAAwC;MACzD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}