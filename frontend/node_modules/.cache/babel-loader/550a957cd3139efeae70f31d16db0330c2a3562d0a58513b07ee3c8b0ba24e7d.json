v>\n    <div v-else>\n      <p>Your cart is empty.</p>\n    </div>\n    <router-link to=\"/user/dashboard\" class=\"btn btn-secondary mt-3\">Back to Dashboard</router-link>\n  </div>\n</template>\n\n<script>\nimport CartService from \"@/services/CartService\";\n\nexport default {\n  data() {\n    return {\n      cartItems: []\n    };\n  },\n  async created() {\n    try {\n      const response = await CartService.getCart();\n      // Assume response.data has a structure like: { items: [...] }\n      this.cartItems = response.data.items || [];\n    } catch (error) {\n      console.error(\"Error fetching cart items:\", error);\n    }\n  },\n  methods: {\n    async removeFromCart(productId) {\n      try {\n        await CartService.removeFromCart(productId);\n        this.cartItems = this.cartItems.filter(item => item.product.id !== productId);\n      } catch (error) {\n        console.error(\"Error removing item from cart:\", error);\n      }\n    }\n  }\n};\n</script>\n"],"mappings":";;AAkCA,OAAOA,WAAU,MAAO,wBAAwB;AAEhD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMJ,WAAW,CAACK,OAAO,CAAC,CAAC;MAC5C;MACA,IAAI,CAACH,SAAQ,GAAIE,QAAQ,CAACH,IAAI,CAACK,KAAI,IAAK,EAAE;IAC5C,EAAE,OAAOC,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;IACpD;EACF,CAAC;EACDE,OAAO,EAAE;IACP,MAAMC,cAAcA,CAACC,SAAS,EAAE;MAC9B,IAAI;QACF,MAAMX,WAAW,CAACU,cAAc,CAACC,SAAS,CAAC;QAC3C,IAAI,CAACT,SAAQ,GAAI,IAAI,CAACA,SAAS,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,OAAO,CAACC,EAAC,KAAMJ,SAAS,CAAC;MAC/E,EAAE,OAAOJ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}