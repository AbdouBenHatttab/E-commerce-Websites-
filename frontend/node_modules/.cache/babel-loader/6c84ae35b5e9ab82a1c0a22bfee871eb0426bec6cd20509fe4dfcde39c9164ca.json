{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AuthService from '@/services/AuthService';\nexport default {\n  name: 'NavBar',\n  data() {\n    return {\n      user: AuthService.getUser()\n    };\n  },\n  computed: {\n    isAdmin() {\n      return this.user && this.user.role === 'ROLE_ADMIN';\n    },\n    isUser() {\n      return this.user && this.user.role === 'ROLE_USER';\n    },\n    profileLink() {\n      // Choose profile route based on role\n      if (this.isAdmin) return '/admin/profile';\n      if (this.isUser) return '/user/profile';\n      return '/login';\n    }\n  },\n  methods: {\n    // Check if the current route is active\n    isActive(route) {\n      return this.$route.path === route;\n    },\n    // Confirm logout to prevent accidental logouts\n    confirmLogout() {\n      if (confirm('Are you sure you want to log out?')) {\n        this.logout();\n      }\n    },\n    // Perform logout and redirect to login page\n    logout() {\n      AuthService.logout();\n      this.user = null;\n      this.$router.push('/login');\n    }\n  },\n  created() {\n    // Listen for storage changes (if login occurs in another tab, for example)\n    window.addEventListener('storage', () => {\n      this.user = AuthService.getUser();\n    });\n  }\n};","map":{"version":3,"names":["AuthService","name","data","user","getUser","computed","isAdmin","role","isUser","profileLink","methods","isActive","route","$route","path","confirmLogout","confirm","logout","$router","push","created","window","addEventListener"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\NavBar.vue"],"sourcesContent":["<template>\n  <nav class=\"navbar navbar-expand-lg navbar-dark bg-primary shadow\">\n    <div class=\"container-fluid\">\n      <router-link class=\"navbar-brand\" to=\"/\">My Ecommerce</router-link>\n      <button\n        class=\"navbar-toggler\"\n        type=\"button\"\n        data-bs-toggle=\"collapse\"\n        data-bs-target=\"#navbarNav\"\n        aria-controls=\"navbarNav\"\n        aria-expanded=\"false\"\n        aria-label=\"Toggle navigation\"\n      >\n        <span class=\"navbar-toggler-icon\"></span>\n      </button>\n      <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\n        <ul class=\"navbar-nav me-auto\">\n          <!-- Public Link (always visible) -->\n          <li class=\"nav-item\">\n            <router-link\n              class=\"nav-link\"\n              to=\"/\"\n              :class=\"{ active: isActive('/') }\"\n              >Home</router-link\n            >\n          </li>\n\n          <!-- Role-based Links -->\n          <template v-if=\"user\">\n            <!-- Common for all logged in users -->\n            <li class=\"nav-item\">\n              <router-link\n                class=\"nav-link\"\n                :to=\"profileLink\"\n                :class=\"{ active: isActive(profileLink) }\"\n                >Profile</router-link\n              >\n            </li>\n\n            <!-- Admin-only links -->\n            <template v-if=\"isAdmin\">\n              <li class=\"nav-item\">\n                <router-link\n                  class=\"nav-link\"\n                  to=\"/admin/dashboard\"\n                  :class=\"{ active: isActive('/admin/dashboard') }\"\n                  >Admin Dashboard</router-link\n                >\n              </li>\n              <li class=\"nav-item\">\n                <router-link\n                  class=\"nav-link\"\n                  to=\"/admin/users\"\n                  :class=\"{ active: isActive('/admin/users') }\"\n                  >Manage Users</router-link\n                >\n              </li>\n              <li class=\"nav-item\">\n                <router-link\n                  class=\"nav-link\"\n                  to=\"/admin/products\"\n                  :class=\"{ active: isActive('/admin/products') }\"\n                  >Manage Products</router-link\n                >\n              </li>\n              <li class=\"nav-item\">\n                <router-link\n                  class=\"nav-link\"\n                  to=\"/admin/messages\"\n                  :class=\"{ active: isActive('/admin/messages') }\"\n                  >Contact Messages</router-link\n                >\n              </li>\n            </template>\n\n            <!-- User-only links -->\n            <template v-else-if=\"isUser\">\n              <li class=\"nav-item\">\n                <router-link\n                  class=\"nav-link\"\n                  to=\"/user/dashboard\"\n                  :class=\"{ active: isActive('/user/dashboard') }\"\n                  >Dashboard</router-link\n                >\n              </li>\n              <li class=\"nav-item\">\n                <router-link\n                  class=\"nav-link\"\n                  to=\"/user/products\"\n                  :class=\"{ active: isActive('/user/products') }\"\n                  >Products</router-link\n                >\n              </li>\n              <li class=\"nav-item\">\n                <router-link\n                  class=\"nav-link\"\n                  to=\"/user/cart\"\n                  :class=\"{ active: isActive('/user/cart') }\"\n                  >My Cart</router-link\n                >\n              </li>\n            </template>\n          </template>\n        </ul>\n\n        <ul class=\"navbar-nav\">\n          <!-- If no user logged in, show Login/Register -->\n          <template v-if=\"!user\">\n            <li class=\"nav-item\">\n              <router-link\n                class=\"nav-link\"\n                to=\"/login\"\n                :class=\"{ active: isActive('/login') }\"\n                >Login</router-link\n              >\n            </li>\n            <li class=\"nav-item\">\n              <router-link\n                class=\"nav-link\"\n                to=\"/register\"\n                :class=\"{ active: isActive('/register') }\"\n                >Register</router-link\n              >\n            </li>\n          </template>\n\n          <!-- If user is logged in, show Logout -->\n          <template v-else>\n            <li class=\"nav-item\">\n              <a class=\"nav-link\" href=\"#\" @click.prevent=\"confirmLogout\">\n                Logout\n              </a>\n            </li>\n          </template>\n        </ul>\n      </div>\n    </div>\n  </nav>\n</template>\n\n<script>\nimport AuthService from '@/services/AuthService';\n\nexport default {\n  name: 'NavBar',\n  data() {\n    return {\n      user: AuthService.getUser(),\n    };\n  },\n  computed: {\n    isAdmin() {\n      return this.user && this.user.role === 'ROLE_ADMIN';\n    },\n    isUser() {\n      return this.user && this.user.role === 'ROLE_USER';\n    },\n    profileLink() {\n      // Choose profile route based on role\n      if (this.isAdmin) return '/admin/profile';\n      if (this.isUser) return '/user/profile';\n      return '/login';\n    },\n  },\n  methods: {\n    // Check if the current route is active\n    isActive(route) {\n      return this.$route.path === route;\n    },\n\n    // Confirm logout to prevent accidental logouts\n    confirmLogout() {\n      if (confirm('Are you sure you want to log out?')) {\n        this.logout();\n      }\n    },\n\n    // Perform logout and redirect to login page\n    logout() {\n      AuthService.logout();\n      this.user = null;\n      this.$router.push('/login');\n    },\n  },\n  created() {\n    // Listen for storage changes (if login occurs in another tab, for example)\n    window.addEventListener('storage', () => {\n      this.user = AuthService.getUser();\n    });\n  },\n};\n</script>\n\n<style scoped>\n.navbar {\n  background-color: #f8f9fa;\n}\n\n.navbar-brand {\n  font-weight: 700;\n  font-size: 1.5rem;\n}\n\n.navbar-nav .nav-item .nav-link.active {\n  color: #007bff;\n  font-weight: bold;\n}\n\n.navbar-toggler {\n  border: none;\n}\n\n.navbar-nav {\n  font-size: 1.1rem;\n}\n\n.navbar-nav .nav-link {\n  transition: color 0.2s;\n}\n\n.navbar-nav .nav-link:hover {\n  color: #0056b3;\n}\n\n@media (max-width: 991px) {\n  .navbar-collapse {\n    background-color: #fff;\n  }\n}\n</style>\n"],"mappings":";AA6IA,OAAOA,WAAU,MAAO,wBAAwB;AAEhD,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAEH,WAAW,CAACI,OAAO,CAAC;IAC5B,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,OAAOA,CAAA,EAAG;MACR,OAAO,IAAI,CAACH,IAAG,IAAK,IAAI,CAACA,IAAI,CAACI,IAAG,KAAM,YAAY;IACrD,CAAC;IACDC,MAAMA,CAAA,EAAG;MACP,OAAO,IAAI,CAACL,IAAG,IAAK,IAAI,CAACA,IAAI,CAACI,IAAG,KAAM,WAAW;IACpD,CAAC;IACDE,WAAWA,CAAA,EAAG;MACZ;MACA,IAAI,IAAI,CAACH,OAAO,EAAE,OAAO,gBAAgB;MACzC,IAAI,IAAI,CAACE,MAAM,EAAE,OAAO,eAAe;MACvC,OAAO,QAAQ;IACjB;EACF,CAAC;EACDE,OAAO,EAAE;IACP;IACAC,QAAQA,CAACC,KAAK,EAAE;MACd,OAAO,IAAI,CAACC,MAAM,CAACC,IAAG,KAAMF,KAAK;IACnC,CAAC;IAED;IACAG,aAAaA,CAAA,EAAG;MACd,IAAIC,OAAO,CAAC,mCAAmC,CAAC,EAAE;QAChD,IAAI,CAACC,MAAM,CAAC,CAAC;MACf;IACF,CAAC;IAED;IACAA,MAAMA,CAAA,EAAG;MACPjB,WAAW,CAACiB,MAAM,CAAC,CAAC;MACpB,IAAI,CAACd,IAAG,GAAI,IAAI;MAChB,IAAI,CAACe,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACAC,MAAM,CAACC,gBAAgB,CAAC,SAAS,EAAE,MAAM;MACvC,IAAI,CAACnB,IAAG,GAAIH,WAAW,CAACI,OAAO,CAAC,CAAC;IACnC,CAAC,CAAC;EACJ;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}