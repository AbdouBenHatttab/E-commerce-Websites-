{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport ProductService from '@/services/ProductService';\nexport default {\n  data() {\n    return {\n      products: [],\n      showForm: false,\n      newProduct: {\n        name: '',\n        description: '',\n        price: 0,\n        stock: 0,\n        imageUrl: ''\n      }\n    };\n  },\n  async created() {\n    try {\n      const response = await ProductService.getAllProducts();\n      this.products = response.data;\n    } catch (error) {\n      console.error('Error fetching products:', error);\n    }\n  },\n  methods: {\n    toggleForm() {\n      this.showForm = !this.showForm;\n    },\n    async addProduct() {\n      try {\n        const response = await ProductService.createProduct(this.newProduct);\n        this.products.push(response.data);\n        this.showForm = false;\n        this.newProduct = {\n          name: '',\n          description: '',\n          price: 0,\n          stock: 0,\n          imageUrl: ''\n        };\n      } catch (error) {\n        console.error('Error adding product:', error);\n      }\n    },\n    async deleteProduct(id) {\n      try {\n        await ProductService.deleteProduct(id);\n        this.products = this.products.filter(product => product.id !== id);\n      } catch (error) {\n        console.error('Error deleting product:', error);\n      }\n    },\n    goBack() {\n      this.$router.push({\n        name: 'AdminDashboard'\n      });\n    }\n  }\n};","map":{"version":3,"names":["ProductService","data","products","showForm","newProduct","name","description","price","stock","imageUrl","created","response","getAllProducts","error","console","methods","toggleForm","addProduct","createProduct","push","deleteProduct","id","filter","product","goBack","$router"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\admin\\AdminProducts.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <h2>Manage Products</h2>\n    <button class=\"btn btn-success mb-3\" @click=\"toggleForm\">\n      Add Product\n    </button>\n\n    <div v-if=\"showForm\" class=\"mb-3\">\n      <input\n        type=\"text\"\n        class=\"form-control mb-2\"\n        v-model=\"newProduct.name\"\n        placeholder=\"Product Name\"\n        required\n      />\n      <input\n        type=\"text\"\n        class=\"form-control mb-2\"\n        v-model=\"newProduct.description\"\n        placeholder=\"Description\"\n        required\n      />\n      <input\n        type=\"number\"\n        class=\"form-control mb-2\"\n        v-model.number=\"newProduct.price\"\n        placeholder=\"Price\"\n        required\n      />\n      <input\n        type=\"number\"\n        class=\"form-control mb-2\"\n        v-model.number=\"newProduct.stock\"\n        placeholder=\"Stock\"\n        required\n      />\n      <input\n        type=\"text\"\n        class=\"form-control mb-2\"\n        v-model=\"newProduct.imageUrl\"\n        placeholder=\"Image URL\"\n      />\n      <button class=\"btn btn-primary\" @click=\"addProduct\">Save Product</button>\n    </div>\n\n    <table class=\"table mt-3\">\n      <thead>\n        <tr>\n          <th>ID</th>\n          <th>Name</th>\n          <th>Description</th>\n          <th>Price</th>\n          <th>Stock</th>\n          <th>Image</th>\n          <th>Actions</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"product in products\" :key=\"product.id\">\n          <td>{{ product.id }}</td>\n          <td>{{ product.name }}</td>\n          <td>{{ product.description }}</td>\n          <td>${{ product.price }}</td>\n          <td>{{ product.stock }}</td>\n          <td>\n            <img\n              :src=\"product.imageUrl\"\n              alt=\"Product Image\"\n              width=\"50\"\n              height=\"50\"\n            />\n          </td>\n          <td>\n            <button\n              class=\"btn btn-danger btn-sm\"\n              @click=\"deleteProduct(product.id)\"\n            >\n              Delete\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n    <button class=\"btn btn-secondary mt-3\" @click=\"goBack\">Back</button>\n  </div>\n</template>\n\n<script>\nimport ProductService from '@/services/ProductService';\n\nexport default {\n  data() {\n    return {\n      products: [],\n      showForm: false,\n      newProduct: {\n        name: '',\n        description: '',\n        price: 0,\n        stock: 0,\n        imageUrl: '',\n      },\n    };\n  },\n  async created() {\n    try {\n      const response = await ProductService.getAllProducts();\n      this.products = response.data;\n    } catch (error) {\n      console.error('Error fetching products:', error);\n    }\n  },\n  methods: {\n    toggleForm() {\n      this.showForm = !this.showForm;\n    },\n    async addProduct() {\n      try {\n        const response = await ProductService.createProduct(this.newProduct);\n        this.products.push(response.data);\n        this.showForm = false;\n        this.newProduct = {\n          name: '',\n          description: '',\n          price: 0,\n          stock: 0,\n          imageUrl: '',\n        };\n      } catch (error) {\n        console.error('Error adding product:', error);\n      }\n    },\n    async deleteProduct(id) {\n      try {\n        await ProductService.deleteProduct(id);\n        this.products = this.products.filter((product) => product.id !== id);\n      } catch (error) {\n        console.error('Error deleting product:', error);\n      }\n    },\n    goBack() {\n      this.$router.push({ name: 'AdminDashboard' });\n    },\n  },\n};\n</script>\n"],"mappings":";;;AAwFA,OAAOA,cAAa,MAAO,2BAA2B;AAEtD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,UAAU,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,WAAW,EAAE,EAAE;QACfC,K