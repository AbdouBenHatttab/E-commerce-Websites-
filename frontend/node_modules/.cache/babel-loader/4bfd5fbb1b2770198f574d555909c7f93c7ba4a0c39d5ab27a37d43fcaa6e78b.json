{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AuthService from '@/services/AuthService';\nexport default {\n  name: 'AdminProfile',\n  data() {\n    return {\n      user: null,\n      userForm: {\n        name: '',\n        email: '',\n        phoneNumber: '',\n        address: ''\n      },\n      isModalVisible: false\n    };\n  },\n  async mounted() {\n    this.fetchAdminProfile();\n  },\n  methods: {\n    async fetchAdminProfile() {\n      try {\n        this.user = AuthService.getCurrentUser();\n        this.userForm = {\n          ...this.user\n        };\n      } catch (error) {\n        console.error('Error fetching admin profile:', error);\n      }\n    },\n    showModal() {\n      this.isModalVisible = true;\n    },\n    closeModal() {\n      this.isModalVisible = false;\n    },\n    async updateProfile() {\n      try {\n        // Implement the logic to update the profile\n        console.log('Profile updated:', this.userForm);\n        this.user = {\n          ...this.userForm\n        };\n        this.closeModal();\n      } catch (error) {\n        console.error('Error updating profile:', error);\n      }\n    },\n    goBack() {\n      this.$router.push({\n        name: 'AdminDashboard'\n      });\n    }\n  }\n};","map":{"version":3,"names":["AuthService","name","data","user","userForm","email","phoneNumber","address","isModalVisible","mounted","fetchAdminProfile","methods","getCurrentUser","error","console","showModal","closeModal","updateProfile","log","goBack","$router","push"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\admin\\AdminProfile.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <h2 class=\"text-center mb-4\">Admin Profile</h2>\n\n    <!-- Loading Spinner -->\n    <div v-if=\"!user\" class=\"d-flex justify-content-center\">\n      <div class=\"spinner-border text-primary\" role=\"status\">\n        <span class=\"visually-hidden\">Loading...</span>\n      </div>\n    </div>\n\n    <!-- Profile Content -->\n    <div v-else>\n      <div class=\"row justify-content-center\">\n        <!-- Profile Image and Basic Info -->\n        <div class=\"col-md-4 mb-4\">\n          <div class=\"card shadow-lg border-0 rounded-4\">\n            <div class=\"card-body text-center\">\n              <img\n                v-if=\"user.imageUrl\"\n                :src=\"user.imageUrl\"\n                class=\"img-fluid rounded-circle shadow-sm\"\n                alt=\"User Image\"\n                style=\"width: 150px; height: 150px; object-fit: cover\"\n              />\n              <img\n                v-else\n                src=\"https://via.placeholder.com/150\"\n                class=\"img-fluid rounded-circle shadow-sm\"\n                alt=\"Default Image\"\n                style=\"width: 150px; height: 150px; object-fit: cover\"\n              />\n              <h4 class=\"mt-3\">{{ user.name }}</h4>\n              <p class=\"text-muted\">{{ user.role }}</p>\n              <p class=\"text-secondary\">{{ user.status }}</p>\n            </div>\n          </div>\n        </div>\n\n        <!-- User Info and Actions -->\n        <div class=\"col-md-6\">\n          <div class=\"card shadow-lg border-0 rounded-4 mb-4\">\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">Personal Information</h5>\n              <ul class=\"list-unstyled\">\n                <li><strong>Email:</strong> {{ user.email }}</li>\n                <li><strong>Phone:</strong> {{ user.phoneNumber || 'N/A' }}</li>\n                <li><strong>Address:</strong> {{ user.address || 'N/A' }}</li>\n              </ul>\n              <button @click=\"showModal\" class=\"btn btn-primary w-100 mt-3\">\n                Edit Profile\n              </button>\n              <button @click=\"goBack\" class=\"btn btn-secondary w-100 mt-2\">\n                Back to Dashboard\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Edit Profile Modal -->\n    <div\n      v-if=\"isModalVisible\"\n      class=\"modal fade show\"\n      tabindex=\"-1\"\n      aria-labelledby=\"editProfileModalLabel\"\n      aria-hidden=\"true\"\n      style=\"display: block\"\n    >\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content rounded-4 shadow-lg\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\" id=\"editProfileModalLabel\">Edit Profile</h5>\n            <button\n              type=\"button\"\n              class=\"btn-close\"\n              @click=\"closeModal\"\n              aria-label=\"Close\"\n            ></button>\n          </div>\n          <div class=\"modal-body\">\n            <form @submit.prevent=\"updateProfile\">\n              <div class=\"mb-3\">\n                <label for=\"name\" class=\"form-label\">Name:</label>\n                <input\n                  id=\"name\"\n                  type=\"text\"\n                  class=\"form-control\"\n                  v-model=\"userForm.name\"\n                  required\n                />\n              </div>\n              <div class=\"mb-3\">\n                <label for=\"email\" class=\"form-label\">Email:</label>\n                <input\n                  id=\"email\"\n                  type=\"email\"\n                  class=\"form-control\"\n                  v-model=\"userForm.email\"\n                  required\n                />\n              </div>\n              <div class=\"mb-3\">\n                <label for=\"phoneNumber\" class=\"form-label\"\n                  >Phone Number:</label\n                >\n                <input\n                  id=\"phoneNumber\"\n                  type=\"text\"\n                  class=\"form-control\"\n                  v-model=\"userForm.phoneNumber\"\n                />\n              </div>\n              <div class=\"mb-3\">\n                <label for=\"address\" class=\"form-label\">Address:</label>\n                <input\n                  id=\"address\"\n                  type=\"text\"\n                  class=\"form-control\"\n                  v-model=\"userForm.address\"\n                />\n              </div>\n              <div class=\"d-flex justify-content-between\">\n                <button type=\"submit\" class=\"btn btn-success w-48\">\n                  Update Profile\n                </button>\n                <button\n                  type=\"button\"\n                  class=\"btn btn-secondary w-48\"\n                  @click=\"closeModal\"\n                >\n                  Cancel\n                </button>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AuthService from '@/services/AuthService';\n\nexport default {\n  name: 'AdminProfile',\n  data() {\n    return {\n      user: null,\n      userForm: {\n        name: '',\n        email: '',\n        phoneNumber: '',\n        address: '',\n      },\n      isModalVisible: false,\n    };\n  },\n  async mounted() {\n    this.fetchAdminProfile();\n  },\n  methods: {\n    async fetchAdminProfile() {\n      try {\n        this.user = AuthService.getCurrentUser();\n        this.userForm = { ...this.user };\n      } catch (error) {\n        console.error('Error fetching admin profile:', error);\n      }\n    },\n    showModal() {\n      this.isModalVisible = true;\n    },\n    closeModal() {\n      this.isModalVisible = false;\n    },\n    async updateProfile() {\n      try {\n        // Implement the logic to update the profile\n        console.log('Profile updated:', this.userForm);\n        this.user = { ...this.userForm };\n        this.closeModal();\n      } catch (error) {\n        console.error('Error updating profile:', error);\n      }\n    },\n    goBack() {\n      this.$router.push({ name: 'AdminDashboard' });\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* General card styles */\n.card {\n  border-radius: 20px;\n  overflow: hidden;\n}\n\n.card-body {\n  padding: 1.5rem;\n}\n\n.card-title {\n  font-weight: 600;\n  font-size: 1.25rem;\n}\n\n.card-text {\n  font-size: 1.1rem;\n}\n\nimg {\n  width: 150px;\n  height: 150px;\n  object-fit: cover;\n  box-shadow: 0px 4px 12px rgba(0, 0, 0, 0.1);\n}\n\n.card-body ul {\n  font-size: 1rem;\n  line-height: 1.8;\n  color: #6c757d;\n}\n\n/* Modal Styling */\n.modal-content {\n  border-radius: 10px;\n  padding: 20px;\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);\n}\n\n.modal-header {\n  padding-bottom: 0;\n}\n\n.modal-body {\n  padding-top: 0;\n}\n\n/* Form Controls */\ninput {\n  font-size: 1.1rem;\n}\n\nlabel {\n  font-weight: 600;\n}\n\nbutton {\n  font-weight: 600;\n  padding: 10px 20px;\n}\n\n.btn-primary,\n.btn-secondary,\n.btn-success {\n  transition: background-color 0.3s ease;\n}\n\n.btn-primary:hover,\n.btn-secondary:hover,\n.btn-success:hover {\n  background-color: #5c6bc0;\n}\n\n/* Button Spacing */\n.d-flex {\n  gap: 10px;\n}\n\n/* Loading Spinner */\n.spinner-border {\n  width: 3rem;\n  height: 3rem;\n}\n\nh2 {\n  font-weight: 600;\n  font-size: 2rem;\n}\n\n/* Responsiveness */\n@media (max-width: 767px) {\n  .card-body ul {\n    font-size: 0.95rem;\n  }\n\n  .btn {\n    font-size: 0.9rem;\n    padding: 8px 16px;\n  }\n\n  .card-img-top {\n    width: 130px;\n    height: 130px;\n  }\n}\n</style>\n"],"mappings":";AAgJA,OAAOA,WAAU,MAAO,wBAAwB;AAEhD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,IAAI;MACVC,QAAQ,EAAE;QACRH,IAAI,EAAE,EAAE;QACRI,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,OAAO,EAAE;MACX,CAAC;MACDC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,IAAI,CAACP,IAAG,GAAIH,WAAW,CAACY,cAAc,CAAC,CAAC;QACxC,IAAI,CAACR,QAAO,GAAI;UAAE,GAAG,IAAI,CAACD;QAAK,CAAC;MAClC,EAAE,OAAOU,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF,