{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AuthService from \"@/services/AuthService\";\nimport 'bootstrap';\nexport default {\n  name: 'AdminProfile',\n  data() {\n    return {\n      user: null,\n      userForm: {\n        name: '',\n        email: ''\n      }\n    };\n  },\n  async mounted() {\n    this.fetchAdminProfile();\n  },\n  methods: {\n    async fetchAdminProfile() {\n      try {\n        this.user = AuthService.getCurrentUser();\n        this.userForm = {\n          ...this.user\n        };\n      } catch (error) {\n        console.error('Error fetching admin profile:', error);\n      }\n    },\n    showModal() {\n      this.$refs.editProfileModal.show();\n    },\n    async updateProfile() {\n      try {\n        // Implement the logic to update the profile\n        console.log('Profile updated:', this.userForm);\n        this.user = {\n          ...this.userForm\n        };\n        this.$refs.editProfileModal.hide();\n      } catch (error) {\n        console.error('Error updating profile:', error);\n      }\n    },\n    goBack() {\n      this.$router.push({\n        name: 'AdminDashboard'\n      });\n    }\n  }\n};","map":{"version":3,"names":["AuthService","name","data","user","userForm","email","mounted","fetchAdminProfile","methods","getCurrentUser","error","console","showModal","$refs","editProfileModal","show","updateProfile","log","hide","goBack","$router","push"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\admin\\AdminProfile.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <h2 class=\"text-center mb-4\">Admin Profile</h2>\n    <div v-if=\"!user\" class=\"alert alert-info\">\n      <p>Loading...</p>\n    </div>\n    <div v-else>\n      <p><strong>Name:</strong> {{ user.name }}</p>\n      <p><strong>Email:</strong> {{ user.email }}</p>\n      <p><strong>Role:</strong> {{ user.role }}</p>\n      <button @click=\"showModal\" class=\"btn btn-primary mt-3\">\n        Edit Profile\n      </button>\n      <button @click=\"goBack\" class=\"btn btn-secondary mt-3 ml-2\">\n        Back to Dashboard\n      </button>\n    </div>\n\n    <!-- Edit Profile Modal -->\n    <div\n      class=\"modal fade\"\n      id=\"editProfileModal\"\n      tabindex=\"-1\"\n      aria-labelledby=\"editProfileModalLabel\"\n      aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\" id=\"editProfileModalLabel\">Edit Profile</h5>\n            <button\n              type=\"button\"\n              class=\"btn-close\"\n              data-bs-dismiss=\"modal\"\n              aria-label=\"Close\"\n            ></button>\n          </div>\n          <div class=\"modal-body\">\n            <form @submit.prevent=\"updateProfile\">\n              <div class=\"mb-3\">\n                <label>Name:</label>\n                <input\n                  type=\"text\"\n                  class=\"form-control\"\n                  v-model=\"userForm.name\"\n                  required\n                />\n              </div>\n              <div class=\"mb-3\">\n                <label>Email:</label>\n                <input\n                  type=\"email\"\n                  class=\"form-control\"\n                  v-model=\"userForm.email\"\n                  required\n                />\n              </div>\n              <button type=\"submit\" class=\"btn btn-success\">\n                Update Profile\n              </button>\n              <button\n                type=\"button\"\n                class=\"btn btn-secondary ml-2\"\n                data-bs-dismiss=\"modal\"\n              >\n                Cancel\n              </button>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AuthService from \"@/services/AuthService\";\nimport 'bootstrap';\n\nexport default {\n  name: 'AdminProfile',\n  data() {\n    return {\n      user: null,\n      userForm: {\n        name: '',\n        email: '',\n      },\n    };\n  },\n  async mounted() {\n    this.fetchAdminProfile();\n  },\n  methods: {\n    async fetchAdminProfile() {\n      try {\n        this.user = AuthService.getCurrentUser();\n        this.userForm = { ...this.user };\n      } catch (error) {\n        console.error('Error fetching admin profile:', error);\n      }\n    },\n    showModal() {\n      this.$refs.editProfileModal.show();\n    },\n    async updateProfile() {\n      try {\n        // Implement the logic to update the profile\n        console.log('Profile updated:', this.userForm);\n        this.user = { ...this.userForm };\n        this.$refs.editProfileModal.hide();\n      } catch (error) {\n        console.error('Error updating profile:', error);\n      }\n    },\n    goBack() {\n      this.$router.push({ name: 'AdminDashboard' });\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your styles here */\n</style>\n"],"mappings":";AA4EA,OAAOA,WAAU,MAAO,wBAAwB;AAChD,OAAO,WAAW;AAElB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,IAAI;MACVC,QAAQ,EAAE;QACRH,IAAI,EAAE,EAAE;QACRI,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,IAAI,CAACJ,IAAG,GAAIH,WAAW,CAACS,cAAc,CAAC,CAAC;QACxC,IAAI,CAACL,QAAO,GAAI;UAAE,GAAG,IAAI,CAACD;QAAK,CAAC;MAClC,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF,CAAC;IACDE,SAASA,CAAA,EAAG;MACV,IAAI,CAACC,KAAK,CAACC,gBAAgB,CAACC,IAAI,CAAC,CAAC;IACpC,CAAC;IACD,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF;QACAL,OAAO,CAACM,GAAG,CAAC,kBAAkB,EAAE,IAAI,CAACb,QAAQ,CAAC;QAC9C,IAAI,CAACD,IAAG,GAAI;UAAE,GAAG,IAAI,CAACC;QAAS,CAAC;QAChC,IAAI,CAACS,KAAK,CAACC,gBAAgB,CAACI,IAAI,CAAC,CAAC;MACpC,EAAE,OAAOR,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD;IACF,CAAC;IACD