{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProductService from '@/services/ProductService';\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      categories: []\n    };\n  },\n  methods: {\n    async fetchCategories() {\n      try {\n        const response = await ProductService.getAllCategories();\n        this.categories = response.data;\n      } catch (error) {\n        console.error('Error fetching categories:', error);\n      }\n    },\n    getCategoryIcon(category) {\n      const icons = {\n        Laptop: 'bi-laptop',\n        Mobile: 'bi-phone',\n        'Mens Fashion': 'bi-person',\n        'Women Fashion': 'bi-person-heart',\n        Electronics: 'bi-cpu',\n        Books: 'bi-book',\n        Home: 'bi-house',\n        Sports: 'bi-trophy'\n      };\n      return icons[category] || 'bi-tag';\n    },\n    getCategoryColor(category) {\n      const colors = {\n        Laptop: 'text-purple',\n        Mobile: 'text-blue',\n        'Mens Fashion': 'text-red',\n        'Women Fashion': 'text-pink',\n        Electronics: 'text-cyan',\n        Books: 'text-orange',\n        Home: 'text-green',\n        Sports: 'text-yellow'\n      };\n      return colors[category] || 'text-muted';\n    },\n    goToAdmin() {\n      this.$router.push({\n        name: 'Login',\n        query: {\n          role: 'ADMIN'\n        }\n      });\n    },\n    goToUser() {\n      this.$router.push({\n        name: 'Login',\n        query: {\n          role: 'USER'\n        }\n      });\n    },\n    hoverEffect(e) {\n      const card = e.currentTarget;\n      const rect = card.getBoundingClientRect();\n      const x = e.clientX - rect.left;\n      const y = e.clientY - rect.top;\n      card.style.setProperty('--x', `${x}px`);\n      card.style.setProperty('--y', `${y}px`);\n    },\n    resetEffect(e) {\n      const card = e.currentTarget;\n      card.style.removeProperty('--x');\n      card.style.removeProperty('--y');\n    }\n  },\n  created() {\n    this.fetchCategories();\n  }\n};","map":{"version":3,"names":["ProductService","name","data","categories","methods","fetchCategories","response","getAllCategories","error","console","getCategoryIcon","category","icons","Laptop","Mobile","Electronics","Books","Home","Sports","getCategoryColor","colors","goToAdmin","$router","push","query","role","goToUser","hoverEffect","e","card","currentTarget","rect","getBoundingClientRect","x","clientX","left","y","clientY","top","style","setProperty","resetEffect","removeProperty","created"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"home-container\">\n    <!-- Role Selection Section -->\n    <section class=\"role-section py-6 bg-gradient\">\n      <div class=\"container\">\n        <div class=\"text-center mb-5\">\n          <h2 class=\"display-5 fw-bold text-dark mb-3\">\n            Choose Your Experience\n          </h2>\n          <p class=\"lead text-muted\">\n            Select your preferred interface to continue\n          </p>\n        </div>\n\n        <div class=\"row g-4 justify-content-center\">\n          <div class=\"col-md-5\">\n            <div\n              class=\"role-card admin-card\"\n              @click=\"goToAdmin\"\n              @mouseenter=\"hoverEffect\"\n              @mouseleave=\"resetEffect\"\n            >\n              <div class=\"role-card-inner\">\n                <div class=\"icon-wrapper bg-primary-blur\">\n                  <i class=\"bi bi-shield-lock-fill\"></i>\n                </div>\n                <h3 class=\"role-title\">Administrator Portal</h3>\n                <p class=\"role-description\">\n                  Full system control with advanced management capabilities\n                </p>\n                <div class=\"hover-indicator\"></div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"col-md-5\">\n            <div\n              class=\"role-card user-card\"\n              @click=\"goToUser\"\n              @mouseenter=\"hoverEffect\"\n              @mouseleave=\"resetEffect\"\n            >\n              <div class=\"role-card-inner\">\n                <div class=\"icon-wrapper bg-success-blur\">\n                  <i class=\"bi bi-person-fill\"></i>\n                </div>\n                <h3 class=\"role-title\">User Dashboard</h3>\n                <p class=\"role-description\">\n                  Personalized shopping experience with account features\n                </p>\n                <div class=\"hover-indicator\"></div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- Categories Section -->\n    <section class=\"categories-section py-6\">\n      <div class=\"container\">\n        <div class=\"text-center mb-5\">\n          <h2 class=\"display-5 fw-bold text-dark mb-3\">Product Categories</h2>\n          <p class=\"lead text-muted\">Explore our curated collections</p>\n        </div>\n\n        <div class=\"row g-4\">\n          <div\n            v-for=\"category in categories\"\n            :key=\"category\"\n            class=\"col-6 col-md-3\"\n          >\n            <router-link\n              :to=\"{ name: 'ProductList', query: { category: category } }\"\n              class=\"category-card\"\n            >\n              <div class=\"category-card-inner\">\n                <div class=\"category-icon\" :class=\"getCategoryColor(category)\">\n                  <i class=\"bi\" :class=\"getCategoryIcon(category)\"></i>\n                </div>\n                <h5 class=\"category-title\">{{ category }}</h5>\n              </div>\n            </router-link>\n          </div>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport ProductService from '@/services/ProductService';\n\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      categories: [],\n    };\n  },\n  methods: {\n    async fetchCategories() {\n      try {\n        const response = await ProductService.getAllCategories();\n        this.categories = response.data;\n      } catch (error) {\n        console.error('Error fetching categories:', error);\n      }\n    },\n    getCategoryIcon(category) {\n      const icons = {\n        Laptop: 'bi-laptop',\n        Mobile: 'bi-phone',\n        'Mens Fashion': 'bi-person',\n        'Women Fashion': 'bi-person-heart',\n        Electronics: 'bi-cpu',\n        Books: 'bi-book',\n        Home: 'bi-house',\n        Sports: 'bi-trophy',\n      };\n      return icons[category] || 'bi-tag';\n    },\n    getCategoryColor(category) {\n      const colors = {\n        Laptop: 'text-purple',\n        Mobile: 'text-blue',\n        'Mens Fashion': 'text-red',\n        'Women Fashion': 'text-pink',\n        Electronics: 'text-cyan',\n        Books: 'text-orange',\n        Home: 'text-green',\n        Sports: 'text-yellow',\n      };\n      return colors[category] || 'text-muted';\n    },\n    goToAdmin() {\n      this.$router.push({ name: 'Login', query: { role: 'ADMIN' } });\n    },\n    goToUser() {\n      this.$router.push({ name: 'Login', query: { role: 'USER' } });\n    },\n    hoverEffect(e) {\n      const card = e.currentTarget;\n      const rect = card.getBoundingClientRect();\n      const x = e.clientX - rect.left;\n      const y = e.clientY - rect.top;\n      card.style.setProperty('--x', `${x}px`);\n      card.style.setProperty('--y', `${y}px`);\n    },\n    resetEffect(e) {\n      const card = e.currentTarget;\n      card.style.removeProperty('--x');\n      card.style.removeProperty('--y');\n    },\n  },\n  created() {\n    this.fetchCategories();\n  },\n};\n</script>\n\n<style scoped>\n.home-container {\n  --purple: #6366f1;\n  --blue: #3b82f6;\n  --red: #ef4444;\n  --pink: #ec4899;\n  --cyan: #06b6d4;\n  --orange: #f59e0b;\n  --green: #10b981;\n  --yellow: #eab308;\n}\n\n.role-section {\n  background: linear-gradient(135deg, #f8fafc 0%, #f1f5f9 100%);\n}\n\n.role-card {\n  position: relative;\n  background: white;\n  border-radius: 1.5rem;\n  padding: 2rem;\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n  overflow: hidden;\n  cursor: pointer;\n}\n\n.role-card::before {\n  content: '';\n  position: absolute;\n  background: radial-gradient(\n    600px circle at var(--x) var(--y),\n    rgba(99, 102, 241, 0.1) 0%,\n    transparent 100%\n  );\n  border-radius: inherit;\n  opacity: 0;\n  transition: opacity 0.3s ease;\n  inset: 0;\n}\n\n.role-card:hover::before {\n  opacity: 1;\n}\n\n.role-card-inner {\n  position: relative;\n  z-index: 2;\n  text-align: center;\n}\n\n.icon-wrapper {\n  width: 80px;\n  height: 80px;\n  border-radius: 1rem;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin: 0 auto 1.5rem;\n  transition: transform 0.3s ease;\n}\n\n.bg-primary-blur {\n  background: rgba(99, 102, 241, 0.1);\n  color: var(--purple);\n}\n\n.bg-success-blur {\n  background: rgba(16, 185, 129, 0.1);\n  color: var(--green);\n}\n\n.role-title {\n  font-weight: 700;\n  margin-bottom: 1rem;\n  color: #1e293b;\n}\n\n.role-description {\n  color: #64748b;\n  font-size: 0.95rem;\n  margin-bottom: 1.5rem;\n}\n\n.category-card {\n  display: block;\n  background: white;\n  border-radius: 1rem;\n  padding: 1.5rem;\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n}\n\n.category-card-inner {\n  text-align: center;\n}\n\n.category-icon {\n  width: 64px;\n  height: 64px;\n  border-radius: 0.75rem;\n  background: rgba(99, 102, 241, 0.1);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin: 0 auto 1rem;\n  font-size: 1.75rem;\n  transition: transform 0.3s ease;\n}\n\n.category-title {\n  font-weight: 600;\n  color: #1e293b;\n  margin: 0;\n}\n\n/* Color variations */\n.text-purple {\n  color: var(--purple);\n}\n.text-blue {\n  color: var(--blue);\n}\n.text-red {\n  color: var(--red);\n}\n.text-pink {\n  color: var(--pink);\n}\n.text-cyan {\n  color: var(--cyan);\n}\n.text-orange {\n  color: var(--orange);\n}\n.text-green {\n  color: var(--green);\n}\n.text-yellow {\n  color: var(--yellow);\n}\n\n/* Interactions */\n.role-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.1);\n}\n\n.category-card:hover {\n  transform: translateY(-3px);\n  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1);\n}\n\n.category-card:hover .category-icon {\n  transform: scale(1.1);\n}\n\n@media (max-width: 768px) {\n  .role-card {\n    padding: 1.5rem;\n  }\n\n  .icon-wrapper {\n    width: 60px;\n    height: 60px;\n    margin-bottom: 1rem;\n  }\n\n  .role-title {\n    font-size: 1.25rem;\n  }\n\n  .category-icon {\n    width: 48px;\n    height: 48px;\n    font-size: 1.5rem;\n  }\n}\n</style>\n"],"mappings":";AA2FA,OAAOA,cAAa,MAAO,2BAA2B;AAEtD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,cAAc,CAACO,gBAAgB,CAAC,CAAC;QACxD,IAAI,CAACJ,UAAS,GAAIG,QAAQ,CAACJ,IAAI;MACjC,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD;IACF,CAAC;IACDE,eAAeA,CAACC,QAAQ,EAAE;MACxB,MAAMC,KAAI,GAAI;QACZC,MAAM,EAAE,WAAW;QACnBC,MAAM,EAAE,UAAU;QAClB,cAAc,EAAE,WAAW;QAC3B,eAAe,EAAE,iBAAiB;QAClCC,WAAW,EAAE,QAAQ;QACrBC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE,UAAU;QAChBC,MAAM,EAAE;MACV,CAAC;MACD,OAAON,KAAK,CAACD,QAAQ,KAAK,QAAQ;IACpC,CAAC;IACDQ,gBAAgBA,CAACR,QAAQ,EAAE;MACzB,MAAMS,MAAK,GAAI;QACbP,MAAM,EAAE,aAAa;QACrBC,MAAM,EAAE,WAAW;QACnB,cAAc,EAAE,UAAU;QAC1B,eAAe,EAAE,WAAW;QAC5BC,WAAW,EAAE,WAAW;QACxBC,KAAK,EAAE,aAAa;QACpBC,IAAI,EAAE,YAAY;QAClBC,MAAM,EAAE;MACV,CAAC;MACD,OAAOE,MAAM,CAACT,QAAQ,KAAK,YAAY;IACzC,CAAC;IACDU,SAASA,CAAA,EAAG;MACV,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEtB,IAAI,EAAE,OAAO;QAAEuB,KAAK,EAAE;UAAEC,IAAI,EAAE;QAAQ;MAAE,CAAC,CAAC;IAChE,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACJ,OAAO,CAACC,IAAI,CAAC;QAAEtB,IAAI,EAAE,OAAO;QAAEuB,KAAK,EAAE;UAAEC,IAAI,EAAE;QAAO;M