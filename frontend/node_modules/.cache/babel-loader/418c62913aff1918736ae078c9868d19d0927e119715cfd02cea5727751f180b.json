{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'ContactUs',\n  data() {\n    return {\n      form: {\n        name: '',\n        email: '',\n        message: ''\n      },\n      statusMessage: '',\n      statusClass: ''\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const response = await axios.post('http://localhost:8080/contact', this.form);\n        if (response.status === 200) {\n          this.statusMessage = 'Your message has been sent successfully!';\n          this.statusClass = 'text-success';\n        } else {\n          this.statusMessage = 'There was an issue with your submission. Please try again.';\n          this.statusClass = 'text-danger';\n        }\n\n        // Reset form\n        this.form.name = '';\n        this.form.email = '';\n        this.form.message = '';\n      } catch (error) {\n        console.error('Error submitting contact form:', error);\n        this.statusMessage = 'An error occurred. Please try again later.';\n        this.statusClass = 'text-danger';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","email","message","statusMessage","statusClass","methods","submitForm","response","post","status","error","console"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\ContactUs.vue"],"sourcesContent":["<template>\n  <div class=\"contact-us-container\">\n    <!-- Gradient Header -->\n    <div class=\"contact-us-header bg-primary-gradient\">\n      <div class=\"container py-5\">\n        <h1 class=\"text-center text-white display-5 fw-bold\">Contact Us</h1>\n      </div>\n    </div>\n\n    <!-- Contact Form Card -->\n    <div class=\"container py-5\">\n      <div class=\"row justify-content-center\">\n        <div class=\"col-md-8 col-lg-6\">\n          <div class=\"card shadow-lg border-0\">\n            <div class=\"card-body p-4 p-md-5\">\n              <form @submit.prevent=\"submitForm\">\n                <!-- Name Input -->\n                <div class=\"form-floating mb-4\">\n                  <input\n                    type=\"text\"\n                    class=\"form-control\"\n                    id=\"name\"\n                    v-model=\"form.name\"\n                    required\n                    placeholder=\"Your Name\"\n                  />\n                  <label for=\"name\">\n                    <i class=\"bi bi-person-fill me-2\"></i>Your Name\n                  </label>\n                </div>\n\n                <!-- Email Input -->\n                <div class=\"form-floating mb-4\">\n                  <input\n                    type=\"email\"\n                    class=\"form-control\"\n                    id=\"email\"\n                    v-model=\"form.email\"\n                    required\n                    placeholder=\"name@example.com\"\n                  />\n                  <label for=\"email\">\n                    <i class=\"bi bi-envelope-fill me-2\"></i>Email Address\n                  </label>\n                </div>\n\n                <!-- Message Input -->\n                <div class=\"form-floating mb-4\">\n                  <textarea\n                    class=\"form-control\"\n                    id=\"message\"\n                    v-model=\"form.message\"\n                    rows=\"4\"\n                    required\n                    placeholder=\"Your Message\"\n                  ></textarea>\n                  <label for=\"message\">\n                    <i class=\"bi bi-chat-left-text-fill me-2\" hi></i>Your Message\n                  </label>\n                </div>\n\n                <!-- Submit Button -->\n                <button type=\"submit\" class=\"btn btn-primary w-100 btn-lg mb-4\">\n                  <i class=\"bi bi-send me-2\"></i>Send Message\n                </button>\n\n                <!-- Alerts -->\n                <div v-if=\"statusMessage\" class=\"alert\" :class=\"statusClass\">\n                  <i class=\"bi\" :class=\"statusIconClass\"></i>{{ statusMessage }}\n                </div>\n              </form>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'ContactUs',\n  data() {\n    return {\n      form: {\n        name: '',\n        email: '',\n        message: '',\n      },\n      statusMessage: '',\n      statusClass: '',\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const response = await axios.post(\n          'http://localhost:8080/contact',\n          this.form\n        );\n\n        if (response.status === 200) {\n          this.statusMessage = 'Your message has been sent successfully!';\n          this.statusClass = 'text-success';\n        } else {\n          this.statusMessage =\n            'There was an issue with your submission. Please try again.';\n          this.statusClass = 'text-danger';\n        }\n\n        // Reset form\n        this.form.name = '';\n        this.form.email = '';\n        this.form.message = '';\n      } catch (error) {\n        console.error('Error submitting contact form:', error);\n        this.statusMessage = 'An error occurred. Please try again later.';\n        this.statusClass = 'text-danger';\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.contact-us-header {\n  background: linear-gradient(135deg, #3b82f6 0%, #1d4ed8 100%);\n  border-radius: 0 0 2rem 2rem;\n}\n\n.card {\n  border-radius: 1.5rem;\n  margin-top: -4rem;\n}\n\n.form-control {\n  border-radius: 0.75rem;\n  padding: 1rem;\n  border: 1px solid #e5e7eb;\n}\n\n.form-control:focus {\n  box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.2);\n  border-color: #3b82f6;\n}\n\n.btn-primary {\n  border-radius: 0.75rem;\n  padding: 1rem;\n  transition: all 0.3s ease;\n}\n\n.btn-primary:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 0.5rem 1rem rgba(59, 130, 246, 0.2);\n}\n\n.alert {\n  border-radius: 0.75rem;\n}\n\n.text-primary {\n  color: #3b82f6 !important;\n}\n\n@media (max-width: 768px) {\n  .card {\n    margin-top: -2rem;\n  }\n\n  .contact-us-header {\n    border-radius: 0 0 1.5rem 1.5rem;\n  }\n}\n</style>\n"],"mappings":"AAgFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJF,IAAI,EAAE,EAAE;QACRG,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX,CAAC;MACDC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAC/B,+BAA+B,EAC/B,IAAI,CAACR,IACP,CAAC;QAED,IAAIO,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACN,aAAY,GAAI,0CAA0C;UAC/D,IAAI,CAACC,WAAU,GAAI,cAAc;QACnC,OAAO;UACL,IAAI,CAACD,aAAY,GACf,4DAA4D;UAC9D,IAAI,CAACC,WAAU,GAAI,aAAa;QAClC;;QAEA;QACA,IAAI,CAACJ,IAAI,CAACF,IAAG,GAAI,EAAE;QACnB,IAAI,CAACE,IAAI,CAACC,KAAI,GAAI,EAAE;QACpB,IAAI,CAACD,IAAI,CAACE,OAAM,GAAI,EAAE;MACxB,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;QACtD,IAAI,CAACP,aAAY,GAAI,4CAA4C;QACjE,IAAI,CAACC,WAAU,GAAI,aAAa;MAClC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}