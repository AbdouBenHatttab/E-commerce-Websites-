button>\n  </div>\n</template>\n\n<script>\nimport ProductService from '@/services/ProductService';\n\nexport default {\n  data() {\n    return {\n      products: [],\n      showForm: false,\n      newProduct: {\n        name: '',\n        description: '',\n        price: 0,\n        stock: 0,\n        imageUrl: '',\n      },\n    };\n  },\n  async created() {\n    const response = await ProductService.getAllProducts();\n    this.products = response.data;\n  },\n  methods: {\n    toggleForm() {\n      this.showForm = !this.showForm;\n    },\n    async addProduct() {\n      const response = await ProductService.createProduct(this.newProduct);\n      this.products.push(response.data);\n      this.showForm = false;\n      this.newProduct = {\n        name: '',\n        description: '',\n        price: 0,\n        stock: 0,\n        imageUrl: '',\n      };\n    },\n    async deleteProduct(id) {\n      await ProductService.deleteProduct(id);\n      this.products = this.products.filter((p) => p.id !== id);\n    },\n    goBack() {\n      this.$router.push({ name: 'AdminDashboard' });\n    },\n  },\n};\n</script>\n"],"mappings":";;;AAuFA,OAAOA,cAAa,MAAO,2BAA2B;AAEtD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,UAAU,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,WAAW,EAAE,EAAE;QACfC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE,CAAC;QACRC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,QAAO,GAAI,MAAMX,cAAc,CAACY,cAAc,CAAC,CAAC;IACtD,IAAI,CAACV,QAAO,GAAIS,QAAQ,CAACV,IAAI;EAC/B,CAAC;EACDY,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACX,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;IAChC,CAAC;IACD,MAAMY,UAAUA,CAAA,EAAG;MACjB,MAAMJ,QAAO,GAAI,MAAMX,cAAc,CAACgB,aAAa,CAAC,IAAI,CAACZ,UAAU,CAAC;MACpE,IAAI,CAACF,QAAQ,CAACe,IAAI,CAACN,QAAQ,CAACV,IAAI,CAAC;MACjC,IAAI,CAACE,QAAO,GAAI,KAAK;MACrB,IAAI,CAACC,UAAS,GAAI;QAChBC,IAAI,EAAE,EAAE;QACRC,WAAW,EAAE,EAAE;QACfC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE,CAAC;QACRC,QAAQ,EAAE;MACZ,CAAC;IACH,CAAC;IACD,MAAMS,aAAaA,CAACC,EAAE,EAAE;M