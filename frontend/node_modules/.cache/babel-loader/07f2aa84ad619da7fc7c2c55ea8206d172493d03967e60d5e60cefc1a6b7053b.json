{"ast":null,"code":"\n/* Script section remains completely unchanged */","map":{"version":3,"names":[],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\admin\\AdminUsers.vue"],"sourcesContent":["<template>\n  <div class=\"container py-4\">\n    <div class=\"card shadow-lg border-0 rounded-3\">\n      <div class=\"card-header bg-primary text-white py-3 rounded-top-3\">\n        <div class=\"d-flex justify-content-between align-items-center\">\n          <h3 class=\"mb-0 fw-semibold\">\n            <i class=\"bi bi-people-fill me-2\"></i>User Management\n          </h3>\n          <button\n            class=\"btn btn-light btn-sm\"\n            @click=\"goBack\"\n            aria-label=\"Go back\"\n          >\n            <i class=\"bi bi-arrow-left me-1\"></i>Back\n          </button>\n        </div>\n      </div>\n\n      <div class=\"card-body p-4\">\n        <div class=\"table-responsive\">\n          <table class=\"table table-hover align-middle mb-0\">\n            <thead class=\"table-light\">\n              <tr>\n                <th class=\"ps-4\">ID</th>\n                <th>Name</th>\n                <th>Email</th>\n                <th>Phone</th>\n                <th>Address</th>\n                <th>Role</th>\n                <th>Status</th>\n                <th class=\"text-end pe-4\">Actions</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr\n                v-for=\"user in users\"\n                :key=\"user.id\"\n                class=\"transition-effect position-relative\"\n              >\n                <td class=\"fw-medium ps-4\">{{ user.id }}</td>\n                <td>\n                  <input\n                    v-model=\"user.name\"\n                    class=\"form-control form-control-sm focus-highlight\"\n                    placeholder=\"Enter name\"\n                  />\n                </td>\n                <td class=\"text-truncate\" style=\"max-width: 200px\">\n                  <span class=\"d-inline-block text-truncate\">\n                    {{ user.email }}\n                  </span>\n                </td>\n                <td>\n                  <input\n                    v-model=\"user.phoneNumber\"\n                    class=\"form-control form-control-sm focus-highlight\"\n                    type=\"tel\"\n                    placeholder=\"Enter phone\"\n                  />\n                </td>\n                <td>\n                  <input\n                    v-model=\"user.address\"\n                    class=\"form-control form-control-sm focus-highlight\"\n                    placeholder=\"Enter address\"\n                  />\n                </td>\n                <td>\n                  <div class=\"d-inline-block position-relative\">\n                    <select\n                      v-model=\"user.role\"\n                      class=\"form-select form-select-sm role-select\"\n                    >\n                      <option value=\"ROLE_USER\">User</option>\n                      <option value=\"ROLE_ADMIN\">Admin</option>\n                    </select>\n                    <span\n                      class=\"role-indicator\"\n                      :class=\"user.role.toLowerCase()\"\n                    ></span>\n                  </div>\n                </td>\n                <td>\n                  <div class=\"d-inline-block position-relative\">\n                    <select\n                      v-model=\"user.status\"\n                      class=\"form-select form-select-sm status-select\"\n                    >\n                      <option value=\"ACTIVE\">Active</option>\n                      <option value=\"INACTIVE\">Inactive</option>\n                    </select>\n                    <span\n                      class=\"status-indicator\"\n                      :class=\"user.status.toLowerCase()\"\n                    ></span>\n                  </div>\n                </td>\n                <td class=\"text-end pe-4\">\n                  <div class=\"d-flex gap-2 justify-content-end\">\n                    <button\n                      class=\"btn btn-sm btn-success action-btn\"\n                      @click=\"updateUser(user)\"\n                      v-if=\"user.status === 'ACTIVE'\"\n                      aria-label=\"Save changes\"\n                    >\n                      <i class=\"bi bi-check-lg me-1\"></i>Save\n                    </button>\n                    <button\n                      class=\"btn btn-sm btn-danger action-btn\"\n                      @click=\"confirmDelete(user.id)\"\n                      aria-label=\"Delete user\"\n                    >\n                      <i class=\"bi bi-trash\"></i>\n                    </button>\n                  </div>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n\n        <div v-if=\"users.length === 0\" class=\"text-center py-5\">\n          <div class=\"text-muted\">\n            <i class=\"bi bi-people display-6\"></i>\n            <p class=\"mt-3\">No users found</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n/* Script section remains completely unchanged */\n</script>\n\n<style scoped>\n.table-hover tbody tr:hover {\n  background-color: rgba(13, 110, 253, 0.03);\n  transform: translateX(4px);\n}\n\n.transition-effect {\n  transition: all 0.25s cubic-bezier(0.4, 0, 0.2, 1);\n}\n\n.card-header {\n  border-bottom: 2px solid rgba(255, 255, 255, 0.15);\n}\n\n.form-control-sm {\n  max-width: 180px;\n  min-width: 140px;\n  transition: border-color 0.2s ease;\n}\n\n.form-select-sm {\n  max-width: 140px;\n  min-width: 120px;\n  appearance: none;\n}\n\n.role-select,\n.status-select {\n  position: relative;\n  padding-left: 28px;\n}\n\n.role-indicator,\n.status-indicator {\n  position: absolute;\n  left: 10px;\n  top: 50%;\n  transform: translateY(-50%);\n  width: 12px;\n  height: 12px;\n  border-radius: 50%;\n  pointer-events: none;\n}\n\n.role-indicator.role_admin {\n  background-color: #0d6efd;\n}\n\n.role-indicator.role_user {\n  background-color: #6c757d;\n}\n\n.status-indicator.active {\n  background-color: #198754;\n}\n\n.status-indicator.inactive {\n  background-color: #dc3545;\n}\n\n.focus-highlight:focus {\n  border-color: #86b7fe;\n  box-shadow: 0 0 0 3px rgba(13, 110, 253, 0.15);\n}\n\n.action-btn {\n  padding: 0.35rem 0.9rem;\n  transition: all 0.2s ease;\n  min-width: 90px;\n}\n\n.action-btn:hover {\n  transform: translateY(-1px);\n  box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);\n}\n\n@media (max-width: 768px) {\n  .table-responsive {\n    border: 1px solid #dee2e6;\n    border-radius: 0.5rem;\n  }\n\n  .form-control-sm {\n    max-width: 120px;\n    min-width: 100px;\n  }\n\n  .action-btn {\n    min-width: auto;\n    padding: 0.3rem 0.7rem;\n  }\n\n  .card-header h3 {\n    font-size: 1.2rem;\n  }\n}\n\n@media (max-width: 576px) {\n  .form-control-sm {\n    max-width: 100px;\n    font-size: 0.8rem;\n  }\n\n  .text-truncate {\n    max-width: 100px;\n  }\n\n  .btn-sm {\n    font-size: 0.75rem;\n  }\n}\n</style>\n"],"mappings":";AAqIA","ignoreList":[]},"metadata":{},"sourceType":"script","externalDependencies":[]}