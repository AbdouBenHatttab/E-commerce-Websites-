{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'ContactMessages',\n  data() {\n    return {\n      messages: []\n    };\n  },\n  async created() {\n    try {\n      const response = await axios.get('/contact/messages');\n      this.messages = response.data;\n    } catch (error) {\n      console.error('Error fetching contact messages:', error);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","messages","created","response","get","error","console"],"sources":["C:\\Users\\Hedib\\OneDrive\\Bureau\\hello-word-vue\\src\\components\\admin\\ContactMessages.vue"],"sourcesContent":["<template>\n  <div class=\"contact-messages-container\">\n    <div class=\"container py-5\">\n      <h1 class=\"text-center\">Contact Messages</h1>\n      <div v-if=\"messages.length === 0\" class=\"alert alert-info text-center\">\n        No messages yet.\n      </div>\n      <ul v-else class=\"list-group\">\n        <li\n          v-for=\"message in messages\"\n          :key=\"message.id\"\n          class=\"list-group-item\"\n        >\n          <h5>\n            {{ message.name }}\n            <span class=\"text-muted\">({{ message.email }})</span>\n          </h5>\n          <p>{{ message.message }}</p>\n          <small class=\"text-muted\">Received on: {{ message.timestamp }}</small>\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'ContactMessages',\n  data() {\n    return {\n      messages: [],\n    };\n  },\n  async created() {\n    try {\n      const response = await axios.get('/contact/messages');\n      this.messages = response.data;\n    } catch (error) {\n      console.error('Error fetching contact messages:', error);\n    }\n  },\n};\n</script>\n\n<style scoped>\n.contact-messages-container {\n  background-color: #f8f9fa;\n  padding-top: 3rem;\n  padding-bottom: 3rem;\n}\n\nh1 {\n  font-size: 2rem;\n  font-weight: bold;\n  color: #333;\n}\n\n.list-group-item {\n  border-radius: 0.75rem;\n  margin-bottom: 1rem;\n}\n\n.text-muted {\n  color: #6c757d !important;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAML,KAAK,CAACM,GAAG,CAAC,mBAAmB,CAAC;MACrD,IAAI,CAACH,QAAO,GAAIE,QAAQ,CAACH,IAAI;IAC/B,EAAE,OAAOK,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;IAC1D;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}