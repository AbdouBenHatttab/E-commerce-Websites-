{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AuthService from '@/services/AuthService';\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      user: null\n    };\n  },\n  mounted() {\n    this.user = AuthService.getCurrentUser();\n  },\n  methods: {\n    goShop() {\n      if (!this.user) {\n        this.$router.push('/login');\n      } else if (this.user.role === 'ROLE_ADMIN') {\n        this.$router.push('/admin/dashboard');\n      } else if (this.user.role === 'ROLE_USER') {\n        this.$router.push('/user/products');\n      } else {\n        this.$router.push('/login');\n      }\n    }\n  }\n};","map":{"version":3,"names":["AuthService","name","data","user","mounted","getCurrentUser","methods","goShop","$router","push","role"],"sources":["C:\\projet stage 2\\final\\website\\frontend\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"home-container\">\n    <!-- Hero Section -->\n    <div class=\"home-hero bg-primary-gradient text-white text-center py-5\">\n      <div class=\"container\">\n        <h1 class=\"display-4 fw-bold\">Welcome to Our Shop</h1>\n        <p class=\"lead mt-3\">\n          Discover quality products at unbeatable prices. Shop now and enjoy a seamless experience.\n        </p>\n\n        <!-- Shop Now button -->\n        <button\n          class=\"btn btn-light btn-lg mt-4 me-3\"\n          @click=\"goShop\"\n        >\n          <i class=\"bi bi-bag-fill me-2\"></i>Shop Now\n        </button>\n\n        <!-- Login button only if NOT logged in -->\n        <router-link\n          v-if=\"!user\"\n          to=\"/login\"\n          class=\"btn btn-outline-light btn-lg mt-4\"\n        >\n          <i class=\"bi bi-box-arrow-in-right me-2\"></i>Login\n        </router-link>\n      </div>\n    </div>\n\n    <!-- Features Section -->\n    <div class=\"container py-5\">\n      <div class=\"row text-center\">\n        <div class=\"col-md-4 mb-4\">\n          <i class=\"bi bi-truck display-4 text-primary mb-3\"></i>\n          <h5>Fast Delivery</h5>\n          <p class=\"text-muted\">We ship nationwide with trusted carriers to ensure your order arrives quickly.</p>\n        </div>\n        <div class=\"col-md-4 mb-4\">\n          <i class=\"bi bi-shield-check display-4 text-primary mb-3\"></i>\n          <h5>Secure Payments</h5>\n          <p class=\"text-muted\">Your transactions are protected with the latest encryption technologies.</p>\n        </div>\n        <div class=\"col-md-4 mb-4\">\n          <i class=\"bi bi-star-fill display-4 text-primary mb-3\"></i>\n          <h5>Top Quality</h5>\n          <p class=\"text-muted\">We offer only the best products, handpicked by our team of experts.</p>\n        </div>\n      </div>\n    </div>\n\n    <!-- CTA -->\n    <div class=\"bg-light py-5\">\n      <div class=\"container text-center\">\n        <h3 class=\"fw-bold mb-3\">New Here?</h3>\n        <p class=\"mb-4 text-muted\">Create an account to start shopping and enjoy exclusive member benefits.</p>\n        <router-link to=\"/register\" class=\"btn btn-primary btn-lg\">\n          <i class=\"bi bi-person-plus-fill me-2\"></i>Register Now\n        </router-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AuthService from '@/services/AuthService'\n\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      user: null,\n    }\n  },\n  mounted() {\n    this.user = AuthService.getCurrentUser()\n  },\n  methods: {\n    goShop() {\n      if (!this.user) {\n        this.$router.push('/login')\n      } else if (this.user.role === 'ROLE_ADMIN') {\n        this.$router.push('/admin/dashboard')\n      } else if (this.user.role === 'ROLE_USER') {\n        this.$router.push('/user/products')\n      } else {\n        this.$router.push('/login')\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.home-hero {\n  border-radius: 0 0 2rem 2rem;\n  background: linear-gradient(135deg, #3b82f6 0%, #1d4ed8 100%);\n}\n\n.btn {\n  border-radius: 0.75rem;\n  padding: 0.75rem 1.5rem;\n  transition: all 0.3s ease;\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  text-decoration: none;\n  cursor: pointer;\n}\n\n.btn:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.1);\n  text-decoration: none;\n}\n\n.bi {\n  color: #1d4ed8;\n}\n\n@media (max-width: 768px) {\n  .home-hero {\n    padding: 3rem 1rem;\n    border-radius: 0 0 1.5rem 1.5rem;\n  }\n}\n</style>\n"],"mappings":";AAgEA,OAAOA,WAAU,MAAO,wBAAuB;AAE/C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACD,IAAG,GAAIH,WAAW,CAACK,cAAc,CAAC;EACzC,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP,IAAI,CAAC,IAAI,CAACJ,IAAI,EAAE;QACd,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC,QAAQ;MAC5B,OAAO,IAAI,IAAI,CAACN,IAAI,CAACO,IAAG,KAAM,YAAY,EAAE;QAC1C,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,kBAAkB;MACtC,OAAO,IAAI,IAAI,CAACN,IAAI,CAACO,IAAG,KAAM,WAAW,EAAE;QACzC,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,gBAAgB;MACpC,OAAO;QACL,IAAI,CAACD,OAAO,CAACC,IAAI,CAAC,QAAQ;MAC5B;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}